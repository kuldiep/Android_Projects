<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kuldiep.myapplication">

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />
 <uses-permission android:name="android.permission.CALL_PHONE"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/xbox"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Calculator" />
        <activity android:name=".Greet_User" />
        <activity android:name=".User_Activity_Registration" />

        <activity android:name=".MainActivity" />
        <activity android:name=".Activity_Navigator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".Common_Activity_News"></activity>
    </application>

</manifest>
